{
  "name": "QA插件",
  "description": "QA插件",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background/background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "declarativeNetRequest",
    "webRequest",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      //      "matches": ["http://gzak.gzsjzyxh.cn/*", "https://gzak.gzsjzyxh.cn/*"],
      "js": [
        "content/config.js",
        "content/data.js",
        "content/content.js",
        "photo/crypto-js.min.js"
      ]
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "web_accessible_resources": [
    {
      "resources": [
        "override.js",
        "hook/hook.js",
        "hook/hook1.js",
        "photo/crypto-js.min.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}